      "use_cases": ["Scene endings", "Isolation", "Scale reveals"]
    },
    "HANDHELD": {
      "name": "Handheld",
      "code": "HANDHELD",
      "description": "Camera operated without stabilization",
      "emotional_impact": "Urgency, realism, chaos, intimacy",
      "use_cases": ["Action", "Documentary feel", "Tension", "Chaos"]
    },
    "STEADICAM": {
      "name": "Steadicam",
      "code": "STEADICAM",
      "description": "Stabilized following shot",
      "emotional_impact": "Smooth pursuit, dreamlike, immersion",
      "use_cases": ["Long takes", "Following subjects", "Exploration"]
    }
