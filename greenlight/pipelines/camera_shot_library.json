      "emotional_impact": "Stability, observation, contemplation",
      "use_cases": ["Dialogue", "Tension building", "Tableaux"]
    },
    "PAN": {
      "name": "Pan",
      "code": "PAN",
      "description": "Camera rotates horizontally on axis",
      "emotional_impact": "Revelation, connection, following action",
      "use_cases": ["Following movement", "Reveals", "Establishing space"]
    },
    "TILT": {
      "name": "Tilt",
      "code": "TILT",
