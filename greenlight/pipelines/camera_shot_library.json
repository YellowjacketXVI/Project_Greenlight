{
  "version": "1.0.0",
  "description": "Comprehensive camera shot library for the Director Pipeline",
  "shot_types": {
    "EWS": {
      "name": "Extreme Wide Shot",
      "code": "EWS",
      "description": "Shows the entire location/environment with subjects as small figures",
      "use_cases": ["Establishing location", "Epic scale", "Isolation", "Journey shots"],
      "emotional_impact": "Awe, isolation, insignificance, grandeur",
      "framing_notes": "Subject occupies less than 10% of frame"
    },
    "WS": {
      "name": "Wide Shot",
      "code": "WS",
      "description": "Full body shot showing subject and surrounding environment",
      "use_cases": ["Scene establishment", "Action sequences", "Group shots", "Dance/movement"],
      "emotional_impact": "Context, orientation, objectivity",
      "framing_notes": "Full body visible with environment context"
    },
    "MWS": {
      "name": "Medium Wide Shot",
      "code": "MWS",
      "description": "Shows subject from knees up with some environment",
      "use_cases": ["Walking conversations", "Casual interactions", "Transitions"],
      "emotional_impact": "Casual observation, natural feel",
      "framing_notes": "Knees to head with background visible"
    },
    "MS": {
      "name": "Medium Shot",
      "code": "MS",
      "description": "Waist-up framing, standard conversational distance",
      "use_cases": ["Dialogue", "Interviews", "Standard coverage", "Reactions"],
      "emotional_impact": "Neutral, conversational, comfortable",
      "framing_notes": "Waist to head, balanced composition"
    },
    "MCU": {
      "name": "Medium Close-Up",
      "code": "MCU",
      "description": "Chest-up framing, more intimate than medium shot",
      "use_cases": ["Important dialogue", "Emotional moments", "Character focus"],
      "emotional_impact": "Engagement, attention, importance",
      "framing_notes": "Chest to head, face prominent"
    },
    "CU": {
      "name": "Close-Up",
      "code": "CU",
      "description": "Face fills most of frame, shows detailed expressions",
      "use_cases": ["Emotional peaks", "Reactions", "Revelations", "Intimate moments"],
      "emotional_impact": "Intimacy, intensity, empathy, tension",
      "framing_notes": "Face fills frame, eyes in upper third"
    },
    "ECU": {
      "name": "Extreme Close-Up",
      "code": "ECU",
      "description": "Single feature (eyes, mouth, hands) fills frame",
      "use_cases": ["Critical details", "Tension building", "Symbolic moments"],
      "emotional_impact": "Intensity, focus, discomfort, significance",
      "framing_notes": "Single feature dominates entire frame"
    },
    "INS": {
      "name": "Insert Shot",
      "code": "INS",
      "description": "Close shot of object or detail relevant to scene",
      "use_cases": ["Props", "Text/documents", "Hands doing action", "Clues"],
      "emotional_impact": "Attention, significance, information",
      "framing_notes": "Object centered, sharp focus"
    },
    "OTS": {
      "name": "Over-the-Shoulder",
      "code": "OTS",
      "description": "Camera behind one subject looking at another",
      "use_cases": ["Dialogue coverage", "Confrontations", "Conversations"],
      "emotional_impact": "Connection, perspective, engagement",
      "framing_notes": "Shoulder/head of foreground subject visible"
    },
    "POV": {
      "name": "Point of View",
      "code": "POV",
      "description": "Camera shows what character sees",
      "use_cases": ["Subjective experience", "Discovery", "Horror/tension", "Empathy"],
      "emotional_impact": "Immersion, identification, vulnerability",
      "framing_notes": "First-person perspective, may include hands"
    },
    "2S": {
      "name": "Two Shot",
      "code": "2S",
      "description": "Two subjects framed together",
      "use_cases": ["Relationships", "Confrontations", "Partnerships", "Romance"],
      "emotional_impact": "Connection, comparison, relationship dynamics",
      "framing_notes": "Both subjects equally prominent"
    },
    "GS": {
      "name": "Group Shot",
      "code": "GS",
      "description": "Three or more subjects framed together",
      "use_cases": ["Team dynamics", "Family scenes", "Meetings", "Ensemble moments"],
      "emotional_impact": "Unity, division, social dynamics",
      "framing_notes": "All subjects visible, hierarchy through positioning"
    }
  },
  "camera_angles": {
    "EYE": {
      "name": "Eye Level",
